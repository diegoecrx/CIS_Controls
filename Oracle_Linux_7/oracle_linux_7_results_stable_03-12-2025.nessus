      if [ -d &quot;$l_mdir/$l_mndir&quot; ] &amp;&amp; [ -n &quot;$(ls -A $l_mdir/$l_mndir)&quot; ]; then
         echo -e &quot;\n - module: \&quot;$l_mname\&quot; exists in \&quot;$l_mdir\&quot;\n - checking if disabled...&quot;
